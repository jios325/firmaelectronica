{
  "name": "firma-oasis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "final-form": "^4.11.1",
    "flexboxgrid": "^6.3.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-final-form": "^4.0.2",
    "react-scripts": "2.1.5",
    "serve": "^10.1.2"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve --single ./build",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"yarn dev\" \"yarn styles:watch\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/index.styl -o ./src/index.css",
    "styles:watch": "stylus -u autoprefixer-stylus -m ./src/index.styl -w ./src/index.styl -o ./src/index.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "concurrently": "^4.1.0",
    "stylus": "^0.54.5"
  }
}
